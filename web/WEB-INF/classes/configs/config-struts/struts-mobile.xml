<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "struts-2.3.dtd">
<struts>
    <package name="mobile" extends="struts-default">
        <action name="queryAllMb" class="mobileAction" method="queryAllMobile">
            <result name="success" type="redirect">index.jsp</result>
        </action>
        <action name="delete" class="mobileAction" method="delMobile">
            <result name="error" type="redirect">index.jsp</result>
        </action>
        <action name="queryOne" class="mobileAction" method="queryOneById">
            <result name="success" type="redirect">update.jsp</result>
        </action>
        <action name="modify" class="mobileAction" method="modifyMobile">
            <result name="input" type="redirect">update.jsp</result>
        </action>
    </package>
</struts>